{
  "name": "@gigwork/precise",
  "version": "0.2.0",
  "description": "Precision based math operations",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsup",
    "format": "prettier --write src",
    "lint": "tsc --noEmit && eslint src",
    "lint:fix": "tsc --noEmit && eslint src --fix",
    "lint:staged": "lint-staged",
    "test": "glob -c \"node --import tsx --test\" \"src/**/*.test.ts\"",
    "prepare": "husky"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "math",
    "precision",
    "finance",
    "arithmetic",
    "decimal",
    "rounding",
    "floating-point",
    "parse number",
    "simple"
  ],
  "author": "Igor Snitkin <sudo@snigo.dev>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@gigwork/eslint-config-service": "^1.0.3",
    "@types/node": "^20.12.6",
    "eslint": "^9.0.0",
    "glob": "^10.3.12",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tslib": "^2.6.2",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.4"
  },
  "imports": {
    "#src/*": "./src/*"
  }
}
